<%= form_with model: reservation, url: reservations_path do |form| %>
  <%= form.hidden_field :custom, value: true%>
  <%= form.label "Fichier CSV" %>
  <%= form.file_field :file, accept: ".csv", class: "form-input" %>
  <%= form.label :num_billet, "Numero billet", class: "form-label" %>
  <%= form.text_field :num_billet, class: "form-input" %>
  <%= form.label :num_reserv, "Réservation", class: "form-label" %>
  <%= form.text_field :num_reserv, class: "form-input" %>
  <%= form.label :date, class: "form-label" %>
  <%= form.text_field :date, class: "form-input" %>
  <%= form.label :hour, "Heure", class: "form-label" %>
  <%= form.text_field :hour, class: "form-input" %>
  <%= form.label :show_key, "Clé spectacle", class: "form-label" %>
  <%= form.text_field :show_key, class: "form-input" %>
  <%= form.label :show, "Spectacle", class: "form-label" %>
  <%= form.text_field :show, class: "form-input" %>
  <%= form.label :represent_key, "Clé représentation", class: "form-label" %>
  <%= form.text_field :represent_key, class: "form-input" %>
  <%= form.label :represent, "Représentation", class: "form-label" %>
  <%= form.text_field :represent, class: "form-input" %>
  <%= form.label :represent_date, "Date représentation", class: "form-label" %>
  <%= form.text_field :represent_date, class: "form-input" %>
  <%= form.label :represent_hour, "Heure représentation", class: "form-label" %>
  <%= form.text_field :represent_hour, class: "form-input" %>
  <%= form.label :represent_end_date, class: "form-label" %>
  <%= form.text_field :represent_end_date, class: "form-input" %>
  <%= form.label :represent_end_hour, class: "form-label" %>
  <%= form.text_field :represent_end_hour, class: "form-input" %>
  <%= form.label :price, "Prix", class: "form-label" %>
  <%= form.text_field :price, class: "form-input" %>
  <%= form.label :product_type, "Type de produit", class: "form-label" %>
  <%= form.text_field :product_type, class: "form-input" %>
  <%= form.label :sales_channel, "Filière de vente", class: "form-label" %>
  <%= form.text_field :sales_channel, class: "form-input" %>
  <%= form.label :lastname, "Nom", class: "form-label" %>
  <%= form.text_field :lastname, class: "form-input" %>
  <%= form.label :firstname, "Prenom", class: "form-label" %>
  <%= form.text_field :firstname, class: "form-input" %>
  <%= form.label :email, class: "form-label" %>
  <%= form.text_field :email, class: "form-input" %>
  <%= form.label :adress, "Adresse", class: "form-label" %>
  <%= form.text_field :adress, class: "form-input" %>
  <%= form.label :pcode, "Code postal", class: "form-label" %>
  <%= form.text_field :pcode, class: "form-input" %>
  <%= form.label :country, "Pays", class: "form-label" %>
  <%= form.text_field :country, class: "form-input" %>
  <%= form.label :age, class: "form-label" %>
  <%= form.text_field :age, class: "form-input" %>
  <%= form.label :gender, "Genre", class: "form-label" %>
  <%= form.text_field :gender, class: "form-input" %>
  <%= form.submit "Importer", class: "btn btn-primary input-group-btn my-2" %>
<% end %>
